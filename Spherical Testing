%calculated Pressures
P1 = run1(3,:)-run1(4,:);
P2 = run2(3,:)-run2(4,:);
P3 = run3(3,:)-run3(4,:);
P4 = run4(3,:)-run4(4,:);
P5 = run5(3,:)-run5(4,:);
P6 = run6(3,:)-run6(4,:);
P7 = run7(3,:)-run7(4,:);
P8 = run8(3,:)-run8(4,:);
P9 = run9(3,:)-run9(4,:);
P10= run10(3,:)-run10(4,:);
P11= run11(3,:)-run11(4,:);
P12= run12(3,:)-run12(4,:);
P13= run13(3,:)-run13(4,:);
P14= run14(3,:)-run14(4,:);
P15= run15(3,:)-run15(4,:);
 
delP=[P1; P2;P3;P4;P5;P6;P7;P8;P9;P10;P11;P12;P13;P14;P15];
delP = abs(delP).*(144);
delP = mean(delP,2);

%calculated   q 
q1 = run1(1,:)-run1(2,:);
q2 = run2(1,:)-run2(2,:);
q3 = run3(1,:)-run3(2,:);
q4 = run4(1,:)-run4(2,:);
q5 = run5(1,:)-run5(2,:);
q6 = run6(1,:)-run6(2,:);
q7 = run7(1,:)-run7(2,:);
q8 = run8(1,:)-run8(2,:);
q9 = run9(1,:)-run9(2,:);
q10 = run10(1,:)-run10(2,:);
q11= run11(1,:)-run11(2,:);
q12= run12(1,:)-run12(2,:);
q13= run13(1,:)-run13(2,:);
q14= run14(1,:)-run14(2,:);
q15= run15(1,:)-run15(2,:);
 
%All the dynamic pressures into one array
Q = [q1; q2; q3; q4; q5; q6; q7; q8; q9; q10; q11 ;q12 ;q13 ;q14 ;q15];
Q = abs(Q).*(144);  
Q = mean(Q,2);

% normalized Pressure 
normP = delP./Q;
normP = abs(normP);
normP = mean(normP,2);

normPt = delPt./Qt;
normPt = abs(normPt);
normPt = mean(normPt,2);

normPf = delPf./Qf;
normPf = abs(normPf);
normPf = mean(normPf,2);

%calculated density
denTest = (Pamb)./(1717.*TempR);
denTestf = (Pamb)./(1717.*TempRf);
denTestt = (Pamb)./(1717.*TempRt);

%calculated viscosity 
mew = ((TempR./Tamb).^(.76)).* 3.74*10^(-7) ;
mewf = ((TempRf./Tamb).^(.76)).* 3.74*10^(-7) ;
mewt = ((TempRt./Tamb).^(.76)).* 3.74*10^(-7) ;

%calculated Wind Speed u in ft/s
u = sqrt((2.*Q)./denTest);
uf = sqrt((2.*Qf)./denTestf);
ut = sqrt((2.*Qt)./denTestt);

%calculated Reynolds Number 
Re = (denTest.*u.*(6/12))./mew;
Ref = (denTestf.*uf.*(4/12))./mewf;
Ret = (denTestt.*ut.*(4.987/12))./mewt;


figure 
grid on
plot(Ref,normPf,'ko',Ret,normPt,'ro',Re,normP,'bo')
hold on 
xlabel('Reynolds Number')
ylabel('Normalized Pressure')
title('Reynolds Number vs Normalized Pressure')
legend('4 inch Sphere','4.987 Inch Sphere','6 Inch Sphere')
